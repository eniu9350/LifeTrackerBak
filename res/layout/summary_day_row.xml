<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/SummaryDay_row_mainlinlo"
	android:layout_width="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:paddingLeft="10sp" android:paddingRight="10sp"
	android:layout_height="wrap_content" android:orientation="vertical"
	android:paddingTop="2sp">
	<TextView android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@+id/SummaryDay_row_tvTargetName"
		android:text="Name" android:typeface="sans" android:textSize="16sp"
		android:textStyle="bold"></TextView>
	<LinearLayout android:id="@+id/SummaryDay_row_sublinlo1"
		android:layout_height="wrap_content" android:layout_width="fill_parent">
		<TextView android:id="@+id/SummaryDay_row_tvCreatedTitle"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:textStyle="bold" android:text="created  " android:textSize="13sp"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Number"
			android:id="@+id/SummaryDay_row_tvCreated" android:textSize="13sp"></TextView>
	</LinearLayout>
	<LinearLayout android:id="@+id/SummaryDay_row_sublinlo2"
		android:layout_height="wrap_content" android:layout_width="fill_parent">
		<TextView android:id="@+id/SummaryDay_row_tvSpecTitle"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:textStyle="bold" android:text="spec " android:textSize="13sp"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Mail"
			android:id="@+id/SummaryDay_row_tvSpec" android:textSize="13sp"
			android:layout_weight="1"></TextView>
	</LinearLayout>

</LinearLayout>